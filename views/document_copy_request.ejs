                    <form class="col-lg-6 mx-auto my-4 bordo-form form_copie" action="add_request_copy_document" method="post"
                        enctype="multipart/form-data">
                        <div class="input-group mb-5">
                            <label for="numero_atto" class="font-weight-bold col-sm-3 p-1">Numero albo atto</label>
                            <input id="numero_atto" type="text" class="form-control text-center" name="numero_atto"
                                placeholder="Numero dell'atto giudiziario" required>
                        </div>
                        <div class="input-group mb-5">
                            <label for="nome_atto" class="font-weight-bold col-sm-3 p-1">Atto giudizario</label>
                            <input id="nome_atto" type="text" class="form-control text-center" name="nome_atto"
                                placeholder="Nome dell'atto giudiziario di cui si vuole la copia">
                        </div>
                        <div class="custom-file">
                            <input type="file" class="custom-file-input" name="ci" id="carta_identita" required>
                            <label class="custom-file-label" for="carta_identita">Copia carta di identità</label>
                        </div>
                        <div class="custom-file mt-5 mb-3">
                            <input type="file" class="custom-file-input" name="cf" id="codice_fiscale" required>
                            <label class="custom-file-label" for="codice_fiscale">Copia codice fiscale</label>
                        </div>
                        <div class="row mt-3 mb-1">
                            <div class="col-sm-3">Autentica:</div>
                            <div class="col-sm-4">
                                <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="autentica" id="inline_autentica1" value="TRUE" required>
                                    <label class="form-check-label" for="inline_autentica1">Si</label>
                                </div>
                                <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="autentica" id="inline_autentica2" value="FALSE" checked>
                                    <label class="form-check-label" for="inline_autentica2">No</label>
                                </div>
                            </div>
                        </div>
                        <div class="row mb-3 mt-1">
                            <div class="col-sm-3">Urgente:</div>
                            <div class="col-sm-4">
                                <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="urgente" id="urgente_inline1" value="TRUE" required>
                                    <label class="form-check-label" for="urgente_inline1">Si</label>
                                </div>
                                <div class="form-check form-check-inline">
                                    <input class="form-check-input" type="radio" name="urgente" id="urgente_inline2" value="FALSE" checked>
                                    <label class="form-check-label" for="urgente_inline2">No</label>
                                </div>
                            </div>
                        </div>
                        <p>Dati non obbligatori:</p>
                        <div class="custom-file mb-5">
                            <input type="file" class="custom-file-input" name="doc_aggiuntivo" id="doc_aggiuntivo" aria-describedby="doc_aggiuntivo">
                            <label class="custom-file-label" for="doc_aggiuntivo">Documento aggiuntivo</label>
                        </div>
                        <div class="text-center mb-2">
                            <button type="submit" class="btn btn-info">Invia richiesta</button>
                        </div>
                    </form>
                    <div id="riepilogo" style="display: none;">
